VIEW "RESCUETIME_TODAY_GROUPED"
AS SELECT
    "APPLICATION",
    "DOCUMENT", 
    SUM("DURATION") AS "DURATION", 
    MIN("SLICETIME") AS "EARLIEST", 
    MAX("SLICETIME") AS "LATEST" 
FROM "RESCUETIME_SLICES_DURATION" 
WHERE TO_TIMESTAMP("SLICETIME") > ADD_SECONDS(NOW(), -86400) 
GROUP BY "APPLICATION", "DOCUMENT"
