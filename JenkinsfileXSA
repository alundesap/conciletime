@Library('piper-library-os') _
node() {
    stage('prepare') {
        checkout scm
        setupCommonPipelineEnvironment script:this, configFile: '.pipeline/configXSA.yml'
    }
    stage('Build') {
        mtaBuild script: this
    }
    stage('deploy') {
        dockerExecute(dockerImage: 'alunde/xsa-cli') {
          sh "xs -v"
        }
        mailSendNotification script: this
    }
}