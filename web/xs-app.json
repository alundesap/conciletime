{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "(catalog/)(.*)",
      "destination": "odata_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(login/)(.*)",
      "destination": "util_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "source": "(callback/)(.*)",
      "destination": "util_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
   {
      "localDir": "resources",
      "source": "^/(.*)",
      "authenticationType": "none"
    }
  ]
}