{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "(catalog/Visits)(.*)",
      "destination": "odata_be",
      "csrfProtection": true,
      "httpMethods": ["GET", "POST"],
      "authenticationType": "none"
    },
    {
        "source": "(catalog/)(.*)",
        "destination": "odata_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
        "source": "(login/)(.*)",
        "destination": "util_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
        "source": "(logout/)(.*)",
        "destination": "util_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
      "source": "(callback/)(.*)",
      "destination": "util_be",
      "csrfProtection": true,
      "authenticationType": "none"
    },
    {
      "source": "(util/)(.*)",
      "destination": "util_be",
      "csrfProtection": true,
      "authenticationType": "none"
    },
    {
      "source": "(app/)(.*)",
      "destination": "app_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
        "source": "(jupyter/)(.*)",
        "destination": "jupyter_be",
        "csrfProtection": true,
        "authenticationType": "none"
    },
    {
        "source": "(mlworker/)(.*)",
        "destination": "mlw_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
      "source": "^/sap/bc/ina/(.*)$",
      "destination": "ina_be",
      "httpMethods": ["POST", "GET"],
      "authenticationType": "xsuaa"
    }, 
    {
      "source": "^/sap/bc/ina/(.*)$",
      "destination": "ina_be",
      "httpMethods": ["OPTIONS"],
      "authenticationType": "none"
    },
    {
      "localDir": "resources",
      "source": "^/(.*)",
      "authenticationType": "none"
    }
  ]
}
