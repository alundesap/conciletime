{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "(catalog/Visits)(.*)",
      "destination": "odata_be",
      "csrfProtection": true,
      "httpMethods": ["GET", "POST"],
      "authenticationType": "none"
    },
    {
        "source": "(catalog/)(.*)",
        "destination": "odata_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
        "source": "(login/)(.*)",
        "destination": "util_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
        "source": "(logout/)(.*)",
        "destination": "util_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
      "source": "(callback/)(.*)",
      "destination": "util_be",
      "csrfProtection": true,
      "authenticationType": "none"
    },
    {
      "source": "(app/)(.*)",
      "destination": "app_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
        "source": "(jupyter/)(.*)",
        "destination": "jupyter_be",
        "csrfProtection": true,
        "authenticationType": "none"
    },
    {
        "source": "(chat/)(.*)",
        "destination": "chat_be",
        "csrfProtection": true,
        "authenticationType": "xsuaa"
    },
    {
        "source": "(socket.io/)(.*)",
        "destination": "chat_be",
        "csrfProtection": true,
        "httpMethods": ["GET", "POST"],
        "authenticationType": "none"
    },
	{
		"source": "/sap/bc/ina/service/v2/GetServerInfo",
		"destination": "ina_be",
		"httpMethods": ["GET"],
		"authenticationType": "none"
	},
    {
		"source": "/sap/bc/ina",
		"destination": "ina_be",
		"httpMethods": ["POST", "GET"],
		"authenticationType": "xsuaa",
		"scope": "$XSAPPNAME.display"
	}, 
	{
		"source": "/sap/bc/ina",
		"destination": "ina_be",
		"httpMethods": ["OPTIONS"],
		"authenticationType": "none"
	},
    {
      "localDir": "resources",
      "source": "^/(.*)",
      "authenticationType": "none"
    }
  ]
}